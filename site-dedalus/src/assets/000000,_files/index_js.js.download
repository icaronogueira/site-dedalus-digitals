function get_mouse_button(e) {
    var evt = (e == null ? event : e);
    evt = evt || window.event;
    var clickType = 'left';
    if (evt.which) {
        if (evt.which==3) clickType='right';
        if (evt.which==2) clickType='middle';
    }else if (evt.button) {
        if (evt.button==2) clickType='right';
        if (evt.button==4) clickType='middle';
    }
    return clickType;
}

