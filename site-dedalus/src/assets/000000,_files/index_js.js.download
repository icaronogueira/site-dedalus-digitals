        if(text){
            text = $.trim(text).replace(/(\r\n|\n|\r|\t)/gm,' ');
            var inverse_keywords_regex = /(crash)[\s.,\-]|(dead)[\s.,\-]|(deaths?)[\s.,\-]|(crash)[\s.,\-]|(collisions?)[\s.,\-]|(colided?)[\s.,\-]|(investigations?)[\s.,\-]|(investigated?)[\s.,\-]|(investigators?)[\s.,\-]|(germanwings?)[\s.,\-]/i;
