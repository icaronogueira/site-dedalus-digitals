        xajax_get_rule_broadcast_options('brd_options_'+guid,value);
    }else if(status == '4'){
        guid = $.guid++;
        var value_spl = value.split(/\|/);
        var email = (value_spl[0]) ? value_spl[0].replace('|', ',') : '';
        value_spl.shift();
        //var shifted = value_spl.join("|");
        //var email_prefix = (shifted) ? shifted.replace(/&#039;/g,'') : '';
