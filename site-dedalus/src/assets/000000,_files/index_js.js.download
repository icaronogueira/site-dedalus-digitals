        if(convert_class != ''){
            if($('#reader_pane').find('.' + convert_class)[0]){
                var tmp_id = $('#reader_pane').find('.' + convert_class).data('aid');
                if(tmp_id && tmp_id != id){
                    $('#reader_pane').find('.' + convert_class).removeClass(convert_class);
