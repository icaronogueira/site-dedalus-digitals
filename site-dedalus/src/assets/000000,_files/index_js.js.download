function detect_rtl_input(input){
    var str = $(input).val();
    var total_length = str.length;
    var arabic_length = 0;
    if(total_length == 0){
        $(input).attr("dir","ltr");
        return;
    }
    for(var i = 0, len = total_length; i < len; i++){
        if(((str.charCodeAt(i) > 1424) && (str.charCodeAt(i) <= 1791))){
            arabic_length++;
        }
    }

    if(total_length > 0 && arabic_length / total_length > 0.1){
        $(input).attr("dir","rtl");
    }else{
        $(input).attr("dir","ltr");
    }
}

